@phdthesis{Schilcher:2006,
  month = {July},
  title = {Implizite Dimensionen des Wissens und ihre Bedeutung f{\"u}r betriebliches Wissensmanagement},
  school = {TU Darmstadt},
  author = {Christian Schilcher},
  year = {2006},
  keywords = {tacit knowledge, knowledge management},
  url = {http://tuprints.ulb.tu-darmstadt.de/716/},
  abstract = {Die Dissertation ist eine Arbeit {\"u}ber Wissen und die Ideen und Versuche, Wissen als {\"o}konomische Ressource im Arbeitskontext einzusetzen und verfolgt das Ziel, einen instruktiven Beitrag zur soziologischen Betrachtung der Konstellation von Wissen und Arbeit zu leisten. Im ersten Teil der Dissertation wird ein anspruchsvolles soziologisches Wissensverst{\"a}ndnis erarbeitet, das auf der Korrektur und Erg{\"a}nzung der herk{\"o}mmlichen Verwendung der Begriffe explizites und implizites Wissen basiert und ein nicht rationalistisch verengtes Wissenskonzept entwirft. Der zweite Teil ist dem Thema Wissensmanagement gewidmet. Dabei werden die {\"U}berlegungen zum Wissen auf Fragen des Umgangs mit Wissen bezogen, um die M{\"o}glichkeiten, Irrt{\"u}mer und Fallstricke dieser neuen Managementmethode einsch{\"a}tzen zu k{\"o}nnen. Das Wissensmanagement l{\"a}sst sich dabei als eine f{\"u}r die neue Wissens{\"o}konomie typische Problemkonstellation auffassen, die in der Zusammenf{\"u}hrung von {\"o}konomischen Rationalit{\"a}ten mit den Logiken des Wissens begr{\"u}ndet liegt.}
}
